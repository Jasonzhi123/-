.nav {
  display: flex;
  height: 80rpx;
  background: #fff;
}

.nav text {
  /* display: block; */
  flex: 1;
  text-align: center;
  line-height: 80rpx;
}

.nav text.cur {
  color: #019dd6;
  border-bottom: 4rpx solid #019dd6;
}

.page01 .bannar {
  width: 100%;
}

.page01 .diantai, .page01 .hotsong {
  margin: 40rpx;
}

.page01 .diantai .title, .page01 .hotsong {
  line-height: 40rpx;
}

.content {
  margin: 30rpx 0rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.content .one {
  flex: 1;
  background: #fff;
}

.content .one:nth-child(2n) {
  margin-left: 20rpx;
}

.content .two {
  margin-bottom: 20rpx;
}

.content .two text {
  width: 300rpx;
  display: block;
  flex: none;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.content .one image {
  width: 100%;
  flex: none;
  height: 250rpx;
}

.content .one text {
  line-height: 50rpx;
  display: block;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.page02 .list {
  display: flex;
  margin: 25rpx 20rpx 20rpx;
  background: #fff;
}

.page02 .list image {
  width: 200rpx;
  height: 200rpx;
  flex: none;
  align-self: center;
  border-radius: 20rpx;
}

.page02 .list .name {
  margin: 5px 30rpx;
  flex: auto;
  font-size: 28rpx;
  line-height: 45rpx;
  overflow: hidden;
}

.page02 .list .name .title {
  font-size: 32rpx;
}

.page02 .list .name .song {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color: #999;
}

.page02 .list .name .song .songname {
  margin: 0rpx 20rpx;
  color: #333;
}

.page02 .list .right {
  width: 30rpx;
  align-self: center;
  flex: none;
}

.page03 .search {
  display: flex;
  margin: 20rpx;
  line-height: 80rpx;
  overflow: hidden;
}
.search-box{
  border: 1px solid #aaa;
 display: flex;
   flex: 6;
   height: 80rpx;
   padding: 0rpx 20rpx;
   border-radius: 30rpx;
}
.page03 .search input {
  flex: 6;
  padding: 0rpx 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  /* border-radius: 25rpx; */
  font-size: 32rpx;
  /* border: 1px solid #aaa; */
}

.page03 .search .button {
  height: 80rpx;
  margin-left: 10rpx;
  border-radius: 5rpx;
  background: transparent;
}

.page03 .search_content {
  height: 400rpx;
  background: #fff;
  padding: 30rpx;
  overflow: hidden;
}

.page03 .search_content .name {
  margin: 40rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

.page03 .search_content .name text {
  padding: 20rpx;
  border: 2rpx solid #999;
  margin: 10rpx;
  border-radius: 25rpx;
  font-size: 24rpx;
  text-align: center;
}

.page03 .search_content .name text:nth-child(1) {
  color: red;
  border-color: red;
  flex: auto;
}

.page03 .result {
  margin: 20rpx;
}

.page03 .result .item {
  display: flex;
  height: 120rpx;
  background: #fff;
  align-items: center;
  padding: 0rpx 20rpx;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}

.page03 .result .item image {
  width: 40rpx;
  height: 40rpx;
}

.page03 .result .item .item-one {
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  font-size: 28rpx;
}

.page03 .result .item .item-one .title {
  font-size: 40rpx;
  line-height: 50rpx;
  width: 100%;
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.page03 .result .item .item-one .songaname {
  font-size: 28rpx;
  line-height: 40rpx;
  display: inline-block;
}

.music {
  width: 100%;
  height: 150rpx;
  background: rgba(0, 0, 0, 0.4);
  position: fixed;
  bottom: 0rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.music .left {
  width: 100rpx;
  height: 100rpx;
  align-self: center;
  flex: none;
  border-radius: 100%;
  margin-left: 20rpx;
}

.music .content {
  display: flex;
  width: 100%;
  flex-direction: column;
  color: #fff;
  margin-left: 30rpx;
}

.music .content .title {
  font-size: 40rpx;
  line-height: 60rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.songname {
  font-size: 26rpx;
}

.music .right {
  width: 100rpx;
  height: 100rpx;
  flex: none;
  margin-right: 20rpx;
}

.rotate {
  animation: roll 3s linear infinite;
}

@keyframes roll {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.footer {
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  width: 100%;
  color: #999;
}

.historyList {
  margin: 5rpx 40rpx 100rpx;
}

.historyList .title {
  display: flex;
  height: 90rpx;
  /* line-height: 100rpx; */
  color: #3a3a3a;
  font-weight: bold;
  font-size: 18px;
  justify-content: space-between;
  align-items: center;
}

.historyList .title-right {
  width: 50rpx;
  height: 50rpx;
}

.historyList .historyList-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px dashed #acacac;
  height: 100rpx;
}

.historyList .historyList-item .item-right {
  width: 50rpx;
  height: 50rpx;
}

.item-left {
  color: #acacac;
  font-weight: "幼圆";
}
